package item;
public class Personaje implements Peleable{

	private double ataque;
	private double defensa;
	
	public Personaje(double ataque, double defensa){
		this.ataque = ataque;
		this.defensa = defensa; 
	}
	
	@Override
	public double getAtaque() {
		return ataque;
	}

	@Override
	public int getEnergia() {
		return energia;
	}
	
	public void setEnergia(int valor) {
		energia = valor;
	}

	@Override
	public double getDefensa() {
		return defensa;
	}
	
}
