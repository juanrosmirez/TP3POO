import item.Peleable;
import personaje.Soldado;
import personaje.Unidad;

public class juego {

	public static void main(String[] args) {
		
		Unidad u1 = new Soldado(9);
		Unidad u2 = new Soldado(9);
		
		System.out.println(u2.conVida());
		for(int i = 0 ; i < 20 ; i++ )
			u1.atacar(u2);
		System.out.println(u2.conVida());
		
	}

}
